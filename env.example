# Configurações da API Groq
VITE_GROQ_API_KEY=your_groq_api_key_here

# Configurações de Criptografia
VITE_ENCRYPTION_SALT=your_secure_salt_here

# Configurações do RAG (Retrieval-Augmented Generation)
VITE_RAG_WEBSITE_URL=https://your-website.com
VITE_RAG_MAX_PAGES=20
VITE_RAG_SEARCH_LIMIT=5
VITE_RAG_MIN_SIMILARITY=0.1
VITE_RAG_CONTEXT_MAX_LENGTH=2000

# Configurações do N8N (se usando)
N8N_API_URL=https://your-n8n-instance.com
N8N_API_KEY=your_n8n_api_key_here

# Configurações do Supabase (se usando)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Configurações do GitHub (se usando)
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here

# Configurações de Desenvolvimento
NODE_ENV=development
VITE_APP_TITLE=Connect 1.1
VITE_APP_VERSION=1.1.0
